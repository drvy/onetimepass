parameters:
    excludePaths:
        - ../vendor/**

    reportUnmatchedIgnoredErrors: false
    treatPhpDocTypesAsCertain: false
    ignoreErrors:
        - '#Call to an undefined static method App\\Models#'
        - '#Interop#'
        - '#Call to an undefined method Psr\\Container\\ContainerInterface::#'
        - '#Access to an undefined property App\\Models\\#'
        - '#Variable \$app might not be defined#'
        - '#Variable \$container might not be defined#'
        - '#Constant ABSPATH not found#'
